{
  "scripts": {
    "test": "jest",
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "prettier": "prettier --write 'src/**/*.js'",
    "lint": "eslint 'src/**/*.js'"
  },
  "dependencies": {
    "@sqorn/pg": "0.0.45",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.5",
    "dotenv-safe": "^6.1.0",
    "elasticsearch": "^16.0.0",
    "fs-router": "^0.4.3",
    "lodash": "^4.17.21",
    "micro": "^9.3.4",
    "micro-compose": "0.0.3",
    "micro-compress": "^1.0.0",
    "micro-cors": "^0.1.1",
    "micro-morgan": "^0.1.0",
    "micro-open-api": "0.0.3",
    "pg": "^7.11.0",
    "pg-native": "^3.0.0",
    "prom-client": "^11.5.2",
    "randomstring": "^1.1.5",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "axios-logger": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-console": 0
    },
    "globals": {
      "testAxios": 0
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "npm run prettier -- ",
        "npm run lint -- ",
        "git add"
      ]
    }
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "tmp",
      "json"
    ],
    "forceExit": true,
    "setupFiles": [
      "./test-axios-create.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "npm ci"
    }
  }
}
