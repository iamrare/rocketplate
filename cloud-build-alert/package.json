{
  "scripts": {},
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-console": 0
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "lint-staged": {
    "linters": {
      "src/**/*.js": [
        "npm run prettier -- ",
        "npm run lint -- ",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-merge": "npm ci"
    }
  }
}
